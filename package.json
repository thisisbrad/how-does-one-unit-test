{
  "name": "how-does-one-unit-test",
  "version": "1.0.0",
  "description": "Basic testing in Node with Mocha",
  "main": "src/app.js",
  "engines": {
    "node": "10.12.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "NODE_ENV=production node src/app.js",
    "dev": "NODE_ENV=development nodemon src/app.js",
    "test": "NODE_ENV=test mocha --recursive './test/**/*.js' --exit --no-deprecation"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thisisbrad/how-does-one-unit-test.git"
  },
  "keywords": [
    "Node",
    "Mocha",
    "Testing"
  ],
  "author": "Brad Beltowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thisisbrad/how-does-one-unit-test/issues"
  },
  "homepage": "https://github.com/thisisbrad/how-does-one-unit-test#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "mongoose": "^5.3.6",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.5",
    "sinon": "^7.1.1",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
